{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior",
          "backdrop-filter",
          "min-width: fit-content",
          "width: fit-content",
          "height: fit-content",
          "min-height: fit-content",
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "disown-opener": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 16.3",
    "not safari <= 16.3"
  ]
}